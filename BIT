#include<bits/stdc++.h>
using namespace std;
int BIT[1000], a[1000], n;
void update(int x, int delta)
{
      for(; x <= n; x += x&-x)
        BIT[x] += delta;
}
int query(int x)
{
    int cnt=0;
     int sum = 0;
     for(; x > 0; x -= x&-x)
    {
        sum += BIT[x];
        cnt++;
    }
    cout<<"No of iterations :"<<cnt;
    cout<<endl;
     return sum;
}

int main()
{
    int t;
    int k;
    scanf("%d", &n);
     int i;
     for(i = 1; i <= n; i++)
     {
           scanf("%d", &a[i]);
           update(i, a[i]);
     }cout<<"no of querries ";
     cin>>t;
while(t>0)
     {cout<<"\nEnter k : ";
     cin>>k;
     printf("sum of first k elements is %d\n", query(k));
     //printf("sum of all elements in range [2, 7] is %d\n", query(7) â€“ query(2-1));
     t--;
     }
     return 0;
}
